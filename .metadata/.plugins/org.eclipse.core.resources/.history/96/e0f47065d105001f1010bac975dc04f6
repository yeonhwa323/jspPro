<script>
function loadXml(){
	sendRequest("ex05_cd_catalog.xml", null, callback, "GET");
}
function callback(){
	if(httpRequest.readyState == 4 ){ // 서버 요청 완료
		if(httpRequest.status == 200) { // 응답성공
			//$("#demo").html(httpRequest.responseText);
			httpRequest.responseXML;
		} else {
			//alert("에이작스 실패 : " + httpRequest.status );
		}
	} 
}

</script>
